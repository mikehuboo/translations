{
  "name": "huboo/translations",
  "description": "This demo package will do some calculations",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Mike Karthauser",
      "email": "mike.karthauser@huboo.com"
    },
    {
      "name": "Peter Smith",
      "email": "peter.smith@huboo.com"
    }
  ],
  "type": "laravel-module",
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.5|^7.0.1",
    "laminas/laminas-json": "^3.1",
    "laravel/framework": "5.8.*|^6|^7.24|^8",
    "spatie/laravel-webhook-client": "^2.8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Huboo\\Translations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Huboo\\Translations\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Huboo\\Translations\\Providers\\TranslationServiceProvider"
      ]
    }
  }
}
